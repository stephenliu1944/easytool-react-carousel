{
  "name": "@easytool/react-carousel",
  "version": "0.1.0",
  "description": "Use for development JS library.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "libraryName": "Carousel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenliu1944/easytool-react-carousel"
  },
  "homepage": "https://github.com/stephenliu1944/easytool-react-carousel",
  "bugs": {
    "url": "https://github.com/stephenliu1944/easytool-react-carousel/issues"
  },
  "keywords": [
    "react",
    "carousel",
    "ellipse",
    "circle"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.dev.js",
    "build:prod": "cross-env NODE_ENV=production rollup -c rollup.config.prod.js",
    "start": "cross-env NODE_ENV=development rollup -c rollup.config.dev.js -w",
    "lint:js": "eslint src/**/*.{js,jsx} -c .eslintrc.prod.json --fix",
    "test": "jest",
    "package": "cross-env BUILD_PATH=dist npm run build:prod && gulp zip",
    "package:watch": "cross-env BUILD_PATH=dist NODE_ENV=production rollup -w -c rollup.config.prod.js",
    "publish:prerelease": "npm run package && gulp version-prerelease && npm publish",
    "publish:patch": "npm run package && gulp version-patch && npm publish",
    "publish:minor": "npm run package && gulp version-minor && npm publish",
    "publish:major": "npm run package && gulp version-major && npm publish",
    "unpublish": "npm unpublish -f",
    "deploy:dev": "npm run package && gulp deploy-dev",
    "deploy:test": "npm run package && gulp deploy-test",
    "deploy:all": "npm run package && gulp deploy-all",
    "git:push": "gulp git-push"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint -c .eslintrc.prod.json --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.5.0",
    "gulp": "^4.0.0",
    "gulp-bump": "^3.1.1",
    "gulp-sftp-up4": "^0.1.8",
    "gulp-zip": "^4.2.0",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^9.3.0",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.23.0",
    "rollup-plugin-alias": "^2.0.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-generate-html-template": "1.5.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-plugin-url": "^2.2.2"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devEnvironments": {
    "servers": {
      "local": 8080
    }
  },
  "deploy": {
    "dev": {
      "host": "",
      "port": 22,
      "user": "",
      "pass": "",
      "zip": true,
      "timeout": 60000,
      "remotePath": "/usr/local/static"
    },
    "test": {
      "host": "",
      "port": 22,
      "user": "",
      "pass": "",
      "zip": true,
      "timeout": 60000,
      "remotePath": "/usr/local/static"
    }
  }
}
